{
  "name": "angular-components",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "ng build angular-components --prod",
    "test": "ng test angular-components",
    "lint": "ng lint angular-components",
    "test:ci": "ng test angular-components --codeCoverage --watch=false --browsers ChromeHeadless --reporters junit",
    "e2e": "ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm start",
    "start": "npm run docs:json && start-storybook -p 43272 -s ./public",
    "compile-css": "node ./build-scripts/compile-css",
    "build-storybook": "build-storybook -c .storybook -o ./dist/storybook -s ./public",
    "generate-storybook-templates": "node ./build-scripts/generate-storybook-scss-module",
    "postinstall": "ngcc",
    "copy-css": "copyfiles --verbose --up 4 --error ./projects/angular-components/src/scss/**/*.scss ./projects/angular-components/dist/scss",
    "dev-mode": "ng build angular-components --watch",
    "after-dev-mode": "npm run copy-css && npm run compile-css"
  },
  "dependencies": {
    "@angular/animations": "9.1.1",
    "@angular/cdk": "9.2.0",
    "@angular/common": "9.1.1",
    "@angular/compiler": "9.1.1",
    "@angular/core": "9.1.1",
    "@angular/forms": "9.1.1",
    "@angular/localize": "9.0.4",
    "@angular/material": "9.2.0",
    "@angular/platform-browser": "9.1.1",
    "@angular/platform-browser-dynamic": "9.1.1",
    "@angular/router": "9.1.1",
    "@ng-select/ng-select": "^4.0.0",
    "autoprefixer": "^8.1.0",
    "bootstrap": "4.3.1",
    "fast-deep-equal": "3.1.1",
    "lottie-web": "5.6.5",
    "moment": "2.24.0",
    "ngx-bootstrap": "5.5.0",
    "ngx-lottie": "5.3.2",
    "rollup": "^1.20.0",
    "rxjs": "6.5.5",
    "tslib": "1.10.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.0",
    "@angular-devkit/build-ng-packagr": "0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.1",
    "@angular/language-service": "9.1.1",
    "@babel/core": "7.13.0",
    "@compodoc/compodoc": "1.1.15",
    "@storybook/addon-actions": "6.3.8",
    "@storybook/addon-controls": "6.3.8",
    "@storybook/addon-docs": "6.3.8",
    "@storybook/addon-knobs": "6.3.1",
    "@storybook/addon-storysource": "6.3.8",
    "@storybook/addon-viewport": "6.3.8",
    "@storybook/angular": "6.3.8",
    "@storybook/source-loader": "6.3.8",
    "@types/jasmine": "3.3.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.11.1",
    "angular-tslint-rules": "1.20.4",
    "babel-loader": "8.0.6",
    "codelyzer": "5.2.2",
    "copyfiles": "2.3.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-junit-reporter": "2.0.1",
    "ng-packagr": "9.1.0",
    "protractor": "5.4.0",
    "ts-node": "7.0.0",
    "tslint": "~6.1.0",
    "typescript": "3.8.3"
  }
}
