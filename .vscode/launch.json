{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to localhost",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "firefox",
      "request": "attach",
      "name": "Attach to FireFox",
      "pathMappings": [
        {
          "url": "webpack:///projects",
          "path": "${workspaceFolder}/projects"
        }
      ]
    }
  ]
}
